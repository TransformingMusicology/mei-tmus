    <xs:attributeGroup ref="mei:att.handident.attributes"/>
    <xs:attributeGroup ref="mei:att.sequence.attributes"/>
  </xs:attributeGroup>
  <xs:group name="model.choicePart">
    <xs:choice>
      <xs:group ref="mei:model.editorialLike"/>
      <xs:group ref="mei:model.editLike"/>
      <xs:element ref="mei:corr"/>
      <xs:element ref="mei:orig"/>
      <xs:element ref="mei:reg"/>
      <xs:element ref="mei:sic"/>
      <xs:element ref="mei:unclear"/>
    </xs:choice>
  </xs:group>
  <xs:group name="model.editLike">
    <xs:choice>
      <xs:element ref="mei:choice"/>
      <xs:element ref="mei:subst"/>
      <xs:element ref="mei:instrName"/>
      <xs:element ref="mei:instrConfig"/>
    </xs:choice>
  </xs:group>
