              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="vol">
                <xs:annotation>
                  <xs:documentation>serial volume.</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
