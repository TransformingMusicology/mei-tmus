      <xs:attributeGroup ref="mei:att.beamSpan.ges.attributes"/>
      <xs:attributeGroup ref="mei:att.beamSpan.anl.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="beatRpt">
    <xs:annotation>
      <xs:documentation>(beat repeat) – An indication that material on a preceding beat should be repeated. </xs:documentation>
