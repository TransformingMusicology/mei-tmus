          </xs:choice>
        </xs:sequence>
        <xs:sequence>
          <xs:element ref="mei:note"/>
          <xs:choice>
            <xs:element ref="mei:chord"/>
            <xs:element ref="mei:note"/>
          </xs:choice>
        </xs:sequence>
      </xs:choice>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.facsimile.attributes"/>
      <xs:attributeGroup ref="mei:att.fTrem.log.attributes"/>
      <xs:attributeGroup ref="mei:att.fTrem.vis.attributes"/>
      <xs:attributeGroup ref="mei:att.fTrem.ges.attributes"/>
      <xs:attributeGroup ref="mei:att.fTrem.anl.attributes"/>
    </xs:complexType>
  </xs:element>
