      <xs:sequence>
        <xs:choice>
          <xs:element ref="mei:music"/>
          <xs:element ref="mei:group"/>
        </xs:choice>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="mei:music"/>
          <xs:element ref="mei:group"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="mei:att.common.attributes"/>
      <xs:attributeGroup ref="mei:att.declaring.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="grpSym">
    <xs:annotation>
      <xs:documentation>(group symbol) – A brace or bracket used to group two or more staves of a score or part. </xs:documentation>
