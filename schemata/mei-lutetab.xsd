      <xs:group ref="mei:model.midiLike"/>
    </xs:choice>
  </xs:group>
  <xs:element name="app">
    <xs:annotation>
      <xs:documentation>(apparatus) – Contains one or more alternative encodings. </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mei:lem"/>
        <xs:element ref="mei:rdg"/>
