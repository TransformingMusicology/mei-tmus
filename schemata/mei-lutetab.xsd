  <xs:attributeGroup name="att.alignment.attribute.when">
    <xs:attribute name="when" type="mei:data.URI">
      <xs:annotation>
        <xs:documentation>indicates the point of occurrence of this feature along a time line. Its value must be the ID of a &lt;when&gt; element elsewhere in the document.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
