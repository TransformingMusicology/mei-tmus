    <xs:attribute name="ornam" type="mei:data.ORNAMS.cmn">
      <xs:annotation>
        <xs:documentation>indicates that this element has an attached ornament. If visual information about the ornament is needed, then one of the elements that represents an ornament (mordent, trill, or turn) should be employed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ornamentaccid.attributes">
    <xs:attributeGroup ref="mei:att.ornamentaccid.attribute.accidupper"/>
    <xs:attributeGroup ref="mei:att.ornamentaccid.attribute.accidlower"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ornamentaccid.attribute.accidupper">
    <xs:attribute name="accidupper" type="mei:data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>records the written accidental associated with an upper neighboring note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.ornamentaccid.attribute.accidlower">
    <xs:attribute name="accidlower" type="mei:data.ACCIDENTAL.EXPLICIT">
      <xs:annotation>
        <xs:documentation>records the written accidental associated with a lower neighboring note.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.trill.anl.attributes">
