    <xs:attribute name="key.accid" type="mei:data.ACCIDENTAL.IMPLICIT">
      <xs:annotation>
        <xs:documentation>contains an accidental for the tonic key, if one is required, e.g., if key.pname equals 'c' and key.accid equals 's', then a tonic of C# is indicated.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.keySigDefault.log.attribute.key.mode">
    <xs:attribute name="key.mode" type="mei:data.MODE">
