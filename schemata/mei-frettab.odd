      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.accidental"/>
        <memberOf key="att.pitched"/>
      </classes>
    </classSpec><classSpec rend="add" ident="att.keyAccid.vis" module="MEI.shared" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Visual domain attributes.</desc>
