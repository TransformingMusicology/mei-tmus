      </remarks><remarks xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <p>This element is modelled on elements in the Encoded Archival Description (EAD) and TEI
          standards.</p>
      </remarks></elementSpec><classSpec rend="add" ident="att.note.ges.tablature" module="MEI.tablature" type="atts"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Gestural domain attributes.</desc><classes/><attList><attDef ident="tab.course" usage="opt">
              <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Records which course is to be played.</desc>
              <datatype xmlns:sch="http://purl.oclc.org/dsdl/schematron">
                <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.COURSENUMBER"/>
              </datatype>
            </attDef><attDef xmlns:sch="http://purl.oclc.org/dsdl/schematron" ident="tab.fret" usage="opt">
          <desc>Records the fret at which a string should be stopped.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.FRETNUMBER"/>
          </datatype>
        </attDef><attDef xmlns:sch="http://purl.oclc.org/dsdl/schematron" ident="tab.string" usage="opt">
          <desc>Records which string is to be played.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.STRINGNUMBER"/>
          </datatype>
        </attDef></attList></classSpec><classSpec rend="add" ident="att.staffDef.ges.tablature" module="MEI.tablature" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Gestural domain attributes for staffDef in tablature.</desc>
      <attList xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <attDef ident="tab.strings" usage="opt">
          <desc>Provides a *written* pitch and octave for each open string or course of
            strings.</desc>
          <datatype>
            <list xmlns="http://relaxng.org/ns/structure/1.0"><oneOrMore><data type="token">
                  <param name="pattern">[a-g][0-9](s|f|ss|x|ff|xs|sx|ts|tf|n|nf|ns|su|sd|fu|fd|nu|nd|1qf|3qf|1qs|3qs)?([a-g][0-9](s|f|ss|x|ff|xs|sx|ts|tf|n|nf|ns|su|sd|fu|fd|nu|nd|1qf|3qf|1qs|3qs)?)*</param>
                </data></oneOrMore></list>
          </datatype>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" predeclare="true" ident="model.backLike" module="MEI.text" type="model">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Groups elements that may contain back matter.</desc>
      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        
      </classes>
    </classSpec><classSpec rend="add" predeclare="true" ident="model.divLike" module="MEI.text" type="model">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Groups elements used to represent generic structural divisions of text.</desc>
    </classSpec><classSpec rend="add" predeclare="true" ident="model.frontLike" module="MEI.text" type="model">
