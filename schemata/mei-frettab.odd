        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><choice><text/><ref name="model.textPhraseLike.limited"/></choice></zeroOrMore>
      </content><attList/></elementSpec><elementSpec rend="add" ident="desc" module="MEI.shared"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">(description) – Container for text that briefly describes the feature to which it is
        attached, including its intended usage, purpose, or application as appropriate.</desc><classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.common"/>
        <memberOf key="att.facsimile"/>
