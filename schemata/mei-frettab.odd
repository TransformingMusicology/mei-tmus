        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><choice><text/><ref name="model.textComponentLike"/><ref name="model.textPhraseLike.limited"/></choice></zeroOrMore>
      </content><constraintSpec xmlns:sch="http://purl.oclc.org/dsdl/schematron" ident="figDesc_content_constraint" scheme="isoschematron">
        <constraint>
          <sch:rule context="mei:figDesc[mei:lg or mei:p or mei:quote or mei:table]">
            <sch:assert test="not(*[../text()[normalize-space()]])">Mixed content is not allowed
              when lg, p, quote, or table is used.</sch:assert>
            <sch:assert test="not(*[not(local-name() eq 'biblList' or local-name() eq 'castList' or                local-name() eq 'lg' or local-name() eq 'list' or local-name() eq 'p' or                local-name() eq 'quote' or local-name() eq 'table')])">Unstructured text not allowed when lg, p, quote, or table elements are
              used.</sch:assert>
