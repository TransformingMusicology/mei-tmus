        <memberOf key="model.eventLike"/>
        <memberOf key="model.staffDefPart"/>
      </classes><content xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <oneOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="clef"/></oneOrMore>
      </content><attList/></elementSpec><elementSpec rend="add" ident="composer" module="MEI.shared"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">The name of the creator of the intellectual content of a musical work.</desc><classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
