        <optional xmlns="http://relaxng.org/ns/structure/1.0"><ref name="relationList"/></optional>
        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="extMeta"/></zeroOrMore>
      </content><attList/></elementSpec><elementSpec rend="add" ident="itemList" module="MEI.frbr"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Gathers bibliographic item entities.</desc><classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.common"/>
