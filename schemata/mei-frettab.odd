        <memberOf key="att.bibl"/>
        <memberOf key="att.facsimile"/>
        <memberOf key="att.lang"/>
        <memberOf key="model.biblPart"/>
      </classes><content xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><choice><text/><ref name="date"/><ref name="model.nameLike"/></choice></zeroOrMore>
      </content><attList/><remarks xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <p>This element is modelled on an element in the Text Encoding Initiative (TEI).</p>
      </remarks></elementSpec><elementSpec rend="add" ident="custos" module="MEI.shared"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Symbol placed at the end of a line of music to indicate the first note of the next line.
        Sometimes called a "direct".</desc><classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
