      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Groups elements that may appear as part of a measure.</desc>
    </classSpec><classSpec rend="add" predeclare="true" ident="model.ossiaLike" module="MEI.cmn" type="model">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Groups elements that function like ossia.</desc>
      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="model.measurePart"/>
        <memberOf key="model.staffPart"/>
