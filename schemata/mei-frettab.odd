      </content>
    </macroSpec><classSpec rend="add" ident="att.arpeg.anl" module="MEI.cmn" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Analytical domain attributes.</desc>
      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.common.anl"/>
      </classes>
    </classSpec><classSpec rend="add" ident="att.arpeg.ges" module="MEI.cmn" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Gestural domain attributes.</desc>
    </classSpec><classSpec rend="add" ident="att.arpeg.log" module="MEI.cmn" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Logical domain attributes.</desc>
      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.controlEvent"/>
        <memberOf key="att.startId"/>
      </classes>
      <attList xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <attDef ident="order" usage="opt">
          <desc>Describes the direction in which an arpeggio is to be performed.</desc>
          <valList type="closed">
            <valItem ident="up">
              <desc>Lowest to highest pitch.</desc>
            </valItem>
            <valItem ident="down">
              <desc>Highest to lowest pitch.</desc>
            </valItem>
            <valItem ident="nonarp">
              <desc>Non-arpeggiated style (usually rendered with a preceding bracket instead of a
                wavy line).</desc>
            </valItem>
          </valList>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" ident="att.arpeg.vis" module="MEI.cmn" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Visual domain attributes.</desc>
      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.altSym"/>
        <memberOf key="att.color"/>
        
        <memberOf key="att.typography"/>
        <memberOf key="att.visualOffset"/>
        <memberOf key="att.xy"/>
      </classes>
      <attList xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <attDef ident="arrow" usage="opt">
          <desc>Indicates if an arrowhead is to be drawn as part of the arpeggiation symbol.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.BOOLEAN"/>
          </datatype>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" ident="att.beam.anl" module="MEI.cmn" type="atts">
