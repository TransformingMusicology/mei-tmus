      <content xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        
      </content>
    </macroSpec><macroSpec rend="add" ident="data.MIDICHANNEL" module="MEI" type="dt">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">MIDI channel numbers.</desc>
      <content xmlns:sch="http://purl.oclc.org/dsdl/schematron">
