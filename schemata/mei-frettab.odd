            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.FRET"/>
          </datatype>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" ident="att.harm.anl" module="MEI.harmony" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Analytical domain attributes.</desc>
