          <sch:rule context="mei:clip[@begin or @end]">
            <sch:assert role="warning" test="@betype or ancestor::mei:*[@betype]">When @begin or
              @end is used, @betype should appear on clip or one of its ancestors.</sch:assert>
          </sch:rule>
        </constraint>
      </constraintSpec><attList/><remarks xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <p>This element is analogous to the <gi scheme="MEI">zone</gi> element in the facsimile
          module.</p>
      </remarks></elementSpec><elementSpec rend="add" ident="performance" module="MEI.performance"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">A presentation of one or more musical works.</desc><classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.common"/>
        <memberOf key="att.declaring"/>
        <memberOf key="model.resourceLike"/>
      </classes><content xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="recording"/></zeroOrMore>
      </content><attList/><remarks xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <p>The <att>decls</att> attribute may be used to link the collection with a particular
          source described in the header. This element is analogous to the <gi scheme="MEI">facsimile</gi> element in the facsimile module.</p>
      </remarks></elementSpec><elementSpec rend="add" ident="recording" module="MEI.performance"><desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">A recorded performance.</desc><classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.common"/>
        <memberOf key="att.dataPointing"/>
        <memberOf key="att.declaring"/>
        <memberOf key="att.mediaBounds"/>
        <memberOf key="att.startId"/>
      </classes><content xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="avFile"/></zeroOrMore>
        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="when"/></zeroOrMore>
