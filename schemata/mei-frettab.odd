      </attList>
    </classSpec><classSpec rend="add" ident="att.accidental.performed" module="MEI.shared" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Attributes for capturing momentary pitch inflection in the gestural domain.</desc>
      <attList xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <attDef ident="accid.ges" usage="opt">
          <desc>Records the performed pitch inflection.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.ACCIDENTAL.IMPLICIT"/>
          </datatype>
          <constraintSpec ident="check_accid_duplication" scheme="isoschematron">
            <constraint>
              <sch:rule context="@accid.ges">
                <sch:assert role="warning" test="not(. eq ../@accid)">The value of @accid.ges should
                  not duplicate the value of @accid.</sch:assert>
              </sch:rule>
            </constraint>
          </constraintSpec>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" ident="att.annot.anl" module="MEI.shared" type="atts">
      <desc xmlns:sch="http://purl.oclc.org/dsdl/schematron">Analytical domain attributes.</desc>
      <classes xmlns:sch="http://purl.oclc.org/dsdl/schematron">
        <memberOf key="att.common.anl"/>
