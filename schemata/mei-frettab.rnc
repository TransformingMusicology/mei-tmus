annot =
  
  ## (annotation) – Provides a short statement explaining the text or indicating the basis for an assertion.
  element annot {
    ((text
      | model.headLike
      | model.textComponentLike
      | model.textPhraseLike
      | model.editLike
      | model.transcriptionLike)*)
    >> sch:pattern [
         id = "mei-annot-annot_content_constraint-constraint-21"
         "\x{a}" ~
         "            "
         sch:rule [
           context =
             "mei:annot[mei:head or mei:lg or mei:p or mei:quote or mei:table]"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "not(mei:head[preceding-sibling::*[not(local-name()='head')]])"
             "Head\x{a}" ~
             "              elements can only occur at the start of annot."
           ]
           "\x{a}" ~
           "               "
           sch:assert [
             test = "not(*[../text()[normalize-space()]])"
             "Mixed content is not allowed\x{a}" ~
             "              when head, lg, p, quote, or table is used."
           ]
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "not(*[not(local-name() eq 'biblList' or local-name() eq 'castList' or local-name() eq 'head' or                local-name() eq 'lg' or local-name() eq 'list' or local-name() eq 'p' or local-name() eq 'quote' or                local-name() eq 'table')])"
             "Unstructured text not allowed when head, lg, p, quote, or table elements are\x{a}" ~
             "              used."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.bibl.attributes,
    att.common.attributes,
    att.facsimile.attributes,
    att.lang.attributes,
    att.source.attributes,
    att.annot.log.attributes,
    att.annot.vis.attributes,
    att.annot.ges.attributes,
    att.annot.anl.attributes,
    att.plist.attributes,
    att.responsibility.attributes,
    att.targetEval.attributes,
    att.typed.attributes,
    empty
  }
